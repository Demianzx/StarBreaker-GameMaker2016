if(mouse_check_button_released(mb_left)){
    if (point_distance(sndx,sndy,mx,my)&lt;70){
        if (global.sonidos=true){
        audio_stop_all()
        global.sonidos=false;
        ini_open("SaveGame.sav");
        ini_write_real("General","Sonido",false);
        ini_close();
        }
        else {global.sonidos=true;
        audio_play_sound(snd_lvls1,50,true);
        ini_open("SaveGame.sav");
        ini_write_real("General","Sonido",true);
        ini_close();
        }
}
}
//boton reset
if(mouse_check_button_released(mb_left)){
    if (point_distance(resx,resy,mx,my)&lt;70){
        room_restart();
        }
}
//boton return
if(mouse_check_button_released(mb_left)){
    if (point_distance(retx,rety,mx,my)&lt;70){
        room_goto(rm_menu);
        }
}
